{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "dom"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noImplicitAny": false,
    "typeRoots": [
      "../../@types",
      "../../node_modules/@types"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src",
    "../../@types",
    "../../node_modules/@types"
  ]
}
